{
  "name": "iwapp",
  "version": "0.0.1",
  "main": "index",
  "description": "Innowatio mobile app",
  "scripts": {
    "build:android": "node node_modules/react-native/local-cli/cli.js run-android --reset-cache",
    "build:ios": "open ios/IwwaMobile.xcodeproj",
    "test": "mocha --compilers js:babel-core/register --require test/setup --recursive test",
    "dev:server": "node node_modules/react-native/local-cli/cli.js start --reset-cache",
    "dev:test": "npm run test -- --watch",
    "dev": "npm-run-all -s config -p dev:*",
    "lint": "eslint app index.* test --ext .js",
    "config": "node scripts/config.js",
    "travis-deploy": "code-push-travis",
    "manual-deploy:cli": "node scripts/deploy-cli.js",
    "manual-deploy": "npm-run-all -s config -p manual-deploy:*"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/innowatio/iwapp.git"
  },
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/innowatio/iwapp/issues"
  },
  "homepage": "https://github.com/innowatio/iwwapp#readme",
  "author": "Team Wattellina <wattellina@mondora.com>",
  "dependencies": {
    "asteroid": "^2.0.2",
    "asteroid-immutable-collections-mixin": "^3.0.0",
    "immutable": "^3.8.1",
    "lodash.debounce": "^4.0.6",
    "lodash.get": "^4.2.1",
    "ramda": "^0.21.0",
    "react": "^0.14.8",
    "react-native": "^0.25.1",
    "react-native-code-push": "^1.11.0-beta",
    "react-native-router-flux": "github:aksonov/react-native-router-flux#0.26-wip",
    "react-redux": "^4.4.5",
    "redux": "^3.5.2",
    "redux-logger": "^2.6.1",
    "redux-thunk": "^2.1.0"
  },
  "devDependencies": {
    "babel-core": "^6.8.0",
    "babel-eslint": "^6.0.4",
    "babel-plugin-transform-es2015-constants": "^6.1.4",
    "babel-preset-airbnb": "^2.0.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react-native": "^1.8.0",
    "babel-preset-stage-0": "^6.5.0",
    "chai": "^3.5.0",
    "chai-enzyme": "^0.4.2",
    "code-push-travis-cli": "^1.0.4",
    "dev-ip": "^1.0.1",
    "enzyme": "^2.3.0",
    "eslint": "^2.10.1",
    "eslint-plugin-babel": "^3.2.0",
    "eslint-plugin-react": "^5.1.1",
    "eslint-plugin-react-native": "^1.0.2",
    "mocha": "^2.4.5",
    "npm-run-all": "^2.1.0",
    "react-addons-test-utils": "^0.14.8",
    "react-dom": "^0.14.8",
    "react-native-cli": "^0.2.0",
    "react-native-mock": "0.1.0",
    "ws": "^1.1.0"
  }
}
